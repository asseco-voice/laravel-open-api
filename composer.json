{
    "name": "asseco-voice/laravel-open-api",
    "description": "Laravel OpenAPI generator",
    "keywords": [
        "php",
        "laravel",
        "openapi"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Voice\\OpenApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voice\\OpenApi\\OpenApiServiceProvider"
            ]
        }
    }
}
