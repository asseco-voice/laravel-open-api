{
    "name": "asseco-voice/laravel-open-api",
    "description": "Laravel OpenAPI generator",
    "keywords": [
        "php",
        "laravel",
        "openapi"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "symfony/yaml": "^6.0",
        "mpociot/reflection-docblock": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^7.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\OpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\OpenApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\OpenApi\\OpenApiServiceProvider"
            ]
        }
    }
}
