{
    "name": "asseco-voice/laravel-open-api",
    "description": "Laravel OpenAPI generator",
    "keywords": [
        "php",
        "laravel",
        "openapi"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "symfony/yaml": "^5.1",
        "mpociot/reflection-docblock": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Asseco\\OpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\OpenApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\OpenApi\\OpenApiServiceProvider"
            ]
        }
    }
}
